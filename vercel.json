{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "config": {
          "nodeVersion": "22.x.x"
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate"
          }
        ]
      }
    ]
  }